msgstr "Styczeń"

#: /home/tim/code/webERP/SystemParameters.php:919
msgid "February"
msgstr "Luty"

#: /home/tim/code/webERP/SystemParameters.php:920
msgid "March"
msgstr "marzec"

#: /home/tim/code/webERP/SystemParameters.php:921
msgid "April"
msgstr "Kwiecień"

#: /home/tim/code/webERP/SystemParameters.php:922
msgid "May"
msgstr "Móc"

#: /home/tim/code/webERP/SystemParameters.php:923
msgid "June"
msgstr "Czerwiec"

#: /home/tim/code/webERP/SystemParameters.php:924
msgid "July"
msgstr "Lipiec"

#: /home/tim/code/webERP/SystemParameters.php:925
msgid "August"
msgstr "Sierpień"

