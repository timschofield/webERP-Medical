    AS '
DECLARE
  pDate ALIAS FOR $1;
  vDays interval;
  vResult integer;
BEGIN
  vDays := (DATE_TRUNC(''day'', pDate) - DATE_TRUNC(''day'', ''0000-01-01''::timestamp));
